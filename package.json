{
  "name": "onoff",
  "version": "3.2.8",
  "description": "GPIO access and interrupt detection with Node.js",
  "main": "onoff.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "lint": "jshint onoff.js examples/*.js integration-test/*.js",
    "test": "nyc mocha",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "coverage-report": "nyc report --reporter=html",
    "integration-test": "cd integration-test && ./run-tests && cd .."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fivdi/onoff.git"
  },
  "homepage": "https://github.com/fivdi/onoff",
  "bugs": {
    "url": "https://github.com/fivdi/onoff/issues"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "epoll": "^2.0.7",
    "lodash.debounce": "^4.0.8"
  },
  "devDependencies": {
    "coveralls": "^3.0.3",
    "jshint": "^2.10.1",
    "mocha": "^5.2.0",
    "nyc": "^13.2.0",
    "mock-fs": "^4.8.0",
    "mock-require": "^3.0.3"
  },
  "keywords": [
    "gpio",
    "embedded",
    "interrupt",
    "beaglebone",
    "raspberry",
    "raspi",
    "rpi",
    "pi",
    "linux"
  ],
  "author": "fivdi",
  "license": "MIT"
}
